{
  "name": "typescript-react-boilerplate",
  "version": "0.0.7",
  "description": "Typescript React Boilerplate",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server --env.development --config webpack/webpack.dev.js",
    "build": "webpack --env.production --config webpack/webpack.prod.js",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "lint:dump": "eslint --print-config ./.eslintrc.js",
    "format": "prettier --write src/**/*.ts{,x}",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache",
      "git add"
    ]
  },
  "keywords": [],
  "author": "sv.volkoff@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.2.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.9",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "autoprefixer": "^9.8.6",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "jest-pnp-resolver": "^1.2.2",
    "jest-resolve": "^26.6.0",
    "lint-staged": "^9.4.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^2.3.4",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.6",
    "typescript": "^4.0.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
