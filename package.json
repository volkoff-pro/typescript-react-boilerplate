{
  "name": "typescript-simple-boilerplate",
  "version": "0.0.1",
  "description": "Typescript Boilerplate",
  "main": "index.ts",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json -t stylish 'src/**/*.{ts,tsx}' --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint -c tslint.json -p tsconfig.json -t stylish --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "sv.volkoff@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.4.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-pnp-resolver": "^1.0.2",
    "jest-resolve": "^23.6.0",
    "lint-staged": "^8.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
